import * as React from 'react'
import { useCallback } from '@hooks'
import { View, Text } from '@components'
import { SectionListData, TItem } from '@typings'
import styles from './styles.css'

type TProps = SectionListData<TItem>
	
const <%= h.changeCase.pascal(name) %>SectionHeader: React.FC<TProps> = ({ title }) => {
