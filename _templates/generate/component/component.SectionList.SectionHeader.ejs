		<View style={styles.sectionHeaderContainer}>
			<Text style={styles.sectionHeaderTitle} numberOfLines={1} ellipsizeMode="tail">{title}</Text>
		</Touchable>
	)
}


export default <%= h.changeCase.pascal(name) %>SectionHeader;
